{
  "name": "testproject-qa-automation-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "initialize": "npm run initialize:env && npm run initialize:database",
    "initialize:env": "cp -n .env.example .env || :",
    "initialize:database": "cp -n prisma/base.sqlite prisma/database.sqlite || :",
    "initialize:revert": "rm -f .env && rm -f prisma/database.sqlite",
    "initialize:fresh": "npm run initialize:revert && npm run initialize",
    "postinstall": "npm run initialize && prisma generate && prisma db push",
    "test:initialize:database": "cp -n prisma/base.sqlite prisma/test.sqlite || :",
    "test:run": "npm run test:initialize:database && cypress run --config-file cypress.config.ts && ts-node scripts/generateReport.ts",
    "test:open": "cypress open --config-file cypress.config.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "@t3-oss/env-nextjs": "^0.3.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.1",
    "@trpc/next": "^10.45.1",
    "@trpc/react-query": "^10.45.1",
    "@trpc/server": "^10.45.1",
    "bcryptjs": "^3.0.2",
    "date-fns": "^2.30.0",
    "gray-matter": "^4.0.3",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.5",
    "nextjs-cors": "^2.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark": "^14.0.3",
    "remark-html": "^15.0.2",
    "superjson": "1.12.2",
    "trpc-openapi": "^1.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/cypress": "^0.1.6",
    "@types/eslint": "^8.56.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.19.129",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^15.3.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^13.5.6",
    "eslint-plugin-deprecation": "^2.0.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.17.0"
  }
}
